/* miniprogram/pages/teskDetail/teskDetail.wxss */
.container{
  width: 100%;
}
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.section{
  width: 90%;
  border-radius: 5px;
  background: #fff;
  margin-top: 10px;
  padding: 10px 5px;
  font-size: 12px;
}
.greenDot,.blueDot,.redDot{
  width:10px;
  height:10px;
  background:green;
  border-radius: 2em;
  margin: 0px 10px 0px 10px;
}
.blueDot{
  background: blue;
}
.redDot{
  background: red;
}