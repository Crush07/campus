/* miniprogram/pages/myAddress/myAddress.wxss */
.container{
  width: 100%;
}
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.addressCard{
  width: 90%;
  height: 80px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  border-radius: 5px;
  font-size: 12px;
  padding: 5px 10px;
  margin: 5px 0px;
}
.cardInfo{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: start;
  align-items: center;
  padding: 5px 0px;
}
.cardInfo view{
  margin: 0px 2px;
}