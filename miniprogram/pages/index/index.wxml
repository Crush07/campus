<view class="container">
  <view class="page-body">
    <view class="page-section page-section-spacing swiper">
      <swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{background}}" wx:key="*this">
          <swiper-item>
            <view class="swiper-item {{item}}"></view>
          </swiper-item>
        </block>
      </swiper>
    </view>
    <van-notice-bar
      color="#1989fa"
      background="#ecf9ff"
      left-icon="volume-o" 
      scrollable
      text="全豹校园帮上线通知！！！"
    />
    <view class="body">
      <view class="modelContainer">
        <image style="width:65px;height:65px" src="image/daiqukuaidi.png" bindtap="express"></image>
        <image style="width:65px;height:65px" src="image/kuaididaiji.png"></image>
        <image style="width:65px;height:65px" src="image/waimaidaina.png"></image>
      </view>
      <view class="modelContainer">
        <image style="width:65px;height:65px" src="image/dayingshangmen.png"></image>
        <image style="width:65px;height:65px" src="image/xiaoyuanpaotui.png" bindtap="errands"></image>
        <image style="width:65px;height:65px" src="image/zizhufuwu.png"></image>
      </view>
      <view class="modelContainer">
        <image style="width:65px;height:65px" src="image/youxipeiwan.png"></image>
        <image style="width:65px;height:65px" src="image/wannengzhujie.png"></image>
        <image style="width:65px;height:65px" src="image/xiaoyuansiji.png"></image>
      </view>
      <view class="modelContainer">
        <view class="bigModel" style="background:#faf2eb;">
          <image style="width:85px;height:85px" src="image/ershoushichang.png"></image>
        </view>
        <view class="bigModel" style="background:#eef0ff;">
          <image style="width:85px;height:85px" src="image/xiaoyuanquanzi.png"></image>
        </view>
      </view>
      <view style="width:100%;height:50px;background:#fff"></view>
    </view>
  </view>
</view>
